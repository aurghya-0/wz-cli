{
  "name": "@aurghyadip/wz-cli",
  "description": "Warzone stats from the comfort of your console",
  "version": "0.2.6",
  "author": "Aurghyadip Kundu",
  "bin": {
    "wz": "./bin/run"
  },
  "bugs": "https://github.com/aurghya-0/wz-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.0.1",
    "call-of-duty-api": "^1.2.0",
    "chalk": "^4.0.0",
    "cli-table3": "^0.6.0",
    "cli-ux": "^5.4.6",
    "csv-writer": "^1.6.0",
    "inquirer": "^7.1.0",
    "keytar": "^6.0.1",
    "makensis": "^0.20.3",
    "r": "0.0.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/bin",
    "/src"
  ],
  "homepage": "https://github.com/aurghya-0/wz-cli",
  "keywords": [
    "oclif",
    "warzone",
    "call of duty"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "bin": "wz",
    "commands": "./src/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "aurghya-0/wz-cli",
  "scripts": {
    "clean": "rm -f .oclif.manifest.json",
    "postpublish": "npm run clean",
    "posttest": "npm run lint",
    "prepack": "oclif-dev manifest",
    "prepublishOnly": "oclif-dev manifest",
    "preversion": "npm run clean",
    "version": "oclif-dev readme && git add README.md"
  }
}
